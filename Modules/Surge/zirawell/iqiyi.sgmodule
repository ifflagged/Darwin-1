[Map Local]
# 播放页开通会员提示
^https?:\/\/act\.vip\.iqiyi\.com\/interact\/api(\/v\d)?\/show data-type=text data="{}" status-code=200

# 播放页多余动效
^https?:\/\/iface2\.iqiyi\.com\/ivos\/interact\/video\/data\? data-type=text data="{}" status-code=200

# 播放页升级白金会员按钮
^https?:\/\/iface2\.iqiyi\.com\/video\/3\.0\/v_interface_proxy\? data-type=text data="{}" status-code=200

# 青少年弹窗
^https?:\/\/iface2\.iqiyi\.com\/views_pop\/3\.0\/pop_control\? data-type=text data="{}" status-code=200

[Script]
# 首页信息流广告
cntv = type=http-response, pattern=^https?:\/\/[\d\.]+\/3f1\/cards\.iqiyi\.com\/(views_home\/3\.0\/qy_home|waterfall\/3\.0\/feed)\?, script-path=https://github.com/ifflagged/Darwin/raw/main/Modules/JS/zirawell/cntv.js, requires-body=true, max-size=-1, timeout=60

cntv = type=http-response, pattern=^https?:\/\/access\.if\.iqiyi\.com\/3f1\/cards\.iqiyi\.com\/(views_category\/3\.0\/category_home|views_home\/3\.0\/qy_home|waterfall\/3\.0\/feed)\?, script-path=https://github.com/ifflagged/Darwin/raw/main/Modules/JS/zirawell/cntv.js, requires-body=true, max-size=-1, timeout=60

cntv = type=http-response, pattern=^https?:\/\/cards\.iqiyi\.com\/views_category\/3\.0\/(category_home|categorylib_content|film_hybrid)\?, script-path=https://github.com/ifflagged/Darwin/raw/main/Modules/JS/zirawell/cntv.js, requires-body=true, max-size=-1, timeout=60

cntv = type=http-response, pattern=^https?:\/\/cards\.iqiyi\.com\/(views_home\/3\.0\/qy_home|waterfall\/3\.0\/feed)\?, script-path=https://github.com/ifflagged/Darwin/raw/main/Modules/JS/zirawell/cntv.js, requires-body=true, max-size=-1, timeout=60

# 播放详情页
cntv = type=http-response, pattern=^https?:\/\/cards\.iqiyi\.com\/views_plt\/3\.0\/player_tabs_v2\?, script-path=https://github.com/ifflagged/Darwin/raw/main/Modules/JS/zirawell/cntv.js, requires-body=true, max-size=-1, timeout=60

# 搜索页列表
cntv = type=http-response, pattern=^https?:\/\/cards\.iqiyi\.com\/views_search\/3\.0\/(hot_query_)?search\?, script-path=https://github.com/ifflagged/Darwin/raw/main/Modules/JS/zirawell/cntv.js, requires-body=true, max-size=-1, timeout=60

# 评论区
cntv = type=http-response, pattern=^https?:\/\/comment-card\.iqiyi\.com\/views_comment\/3\.0\/long_video_comments\?, script-path=https://github.com/ifflagged/Darwin/raw/main/Modules/JS/zirawell/cntv.js, requires-body=true, max-size=-1, timeout=60

# 我的页面菜单
cntv = type=http-response, pattern=^https?:\/\/iface2\.iqiyi\.com\/aggregate\/3\.0\/getMyMenus\?, script-path=https://github.com/ifflagged/Darwin/raw/main/Modules/JS/zirawell/cntv.js, requires-body=true, max-size=-1, timeout=60

# 首页左上角天气
cntv = type=http-response, pattern=^https?:\/\/iface2\.iqiyi\.com\/control\/3\.0\/init_proxy\?, script-path=https://github.com/ifflagged/Darwin/raw/main/Modules/JS/zirawell/cntv.js, requires-body=true, max-size=-1, timeout=60

# 通用控制,各tab页二楼
cntv = type=http-response, pattern=^https?:\/\/iface2\.iqiyi\.com\/fusion\/3\.0\/common_switch\?, script-path=https://github.com/ifflagged/Darwin/raw/main/Modules/JS/zirawell/cntv.js, requires-body=true, max-size=-1, timeout=60

# 底部tab,顶部tab
cntv = type=http-response, pattern=^https?:\/\/iface2\.iqiyi\.com\/views\/3\.0\/(bottom_theme|home_top_menu)\?, script-path=https://github.com/ifflagged/Darwin/raw/main/Modules/JS/zirawell/cntv.js, requires-body=true, max-size=-1, timeout=60

# 搜索框填充词
cntv = type=http-response, pattern=^https?:\/\/search\.video\.iqiyi\.com\/q\?, script-path=https://github.com/ifflagged/Darwin/raw/main/Modules/JS/zirawell/cntv.js, requires-body=true, max-size=-1, timeout=60

# 开屏广告,播放广告
cntv = type=http-response, pattern=^https?:\/\/(kjp|t7z)\.cupid\.iqiyi\.com\/mixer\?, script-path=https://github.com/ifflagged/Darwin/raw/main/Modules/JS/zirawell/cntv.js, requires-body=true, max-size=-1, timeout=60

