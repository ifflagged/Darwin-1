[General]
skip-proxy = %APPEND% iosapps.itunes.apple.com

[Script]
body_rewrite_16 = type=http-request, pattern=^https?:\/\/testflight\.apple\.com\/v\d\/accounts\/.+?\/install$, script-path=https://raw.githubusercontent.com/Script-Hub-Org/Script-Hub/main/scripts/body-rewrite.js, requires-body=true, max-size=-1, timeout=30, argument=%5B%5B%22replace-regex%22%2C%5B%5D%5D%5D

[MITM]
hostname = %APPEND% testflight.apple.com

