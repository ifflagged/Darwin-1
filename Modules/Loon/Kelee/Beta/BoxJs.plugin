[Rule]
DOMAIN-SUFFIX,jsdelivr.net,PROXY

[Script]
http-request ^https?:\/\/(.+\.)?boxjs\.(com|net) script-path=https://github.com/ifflagged/Darwin/raw/main/Modules/JS/Kelee/BoxJs/chavy.boxjs.js, requires-body=true, timeout=120, tag=BoxJS

[MITM]
hostname = boxjs.com, boxjs.net, *.boxjs.com, *.boxjs.net

